#include<iostream>
using namespace std;
int a[100001];
int main(){
    int n, x;
    cout << "nhap so luong phan tu trong day: "; 
    cin >> n;
    for (int i = 0; i < n; i++){
        cin >> a[i];
    }
    cout << "nhap so x can tim: "; 
    cin >> x;
    int count = 0;
    for (int i = 0; i < n; i++){
        if (a[i] == x){
            count ++;
        }
    }
    cout << "so phan tu x co trong day la: " << count;
    return 0;
}
